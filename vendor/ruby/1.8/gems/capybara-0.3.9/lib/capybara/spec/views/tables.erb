<form action="/form" method="post">
  <table id="agent_table">
    <caption>Agent</caption>
  
    <tr>
      <td>
        <label for="form_agent_name">Name</label>
      </td>
      <td>
        <input type="text" name="form[agent_name]" value="James" id="form_agent_name"/>
