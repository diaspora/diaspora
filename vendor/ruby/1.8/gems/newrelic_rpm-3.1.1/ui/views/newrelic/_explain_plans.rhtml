<% if @row_headers %>
  <% @row_headers.length.times do |r| %>
	<% if @row_headers[r] %>
		<tr>
			<th align=right nowrap>
				<%= @row_headers[r]%>
			</th>
			<% @explanation.each do |exp| %>
				<td>
					<%= exp[r]%>
				</td>
			<% end %>
		</tr>
	<% end %>
  <% end %>
<% else %>
  <tr>
  <th align=right>Plan</th>
  <td><p class="plan">
  <% @explanation.each do |row| %>
      <% row.each do |col| %>
<%= h(col) %>
      <% end %>
  <% end %>
  </p></td>
  </tr>
<% end %>
